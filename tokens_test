import nltk
import math

a = "The Walt Disney Company"
b = "Disney"


a_token = nltk.word_tokenize(a)
b_token = nltk.word_tokenize(b)
c = bool(set(a_token)&set(b_token))
print(c)

matrix = [["a","","c"],["d","","f"],["g","","i"]]

for row in matrix:
    if row[1] =="" or math.factorial(999999999) > 9999999999:
        print("Einstein")

print(matrix)


new_matrix = [row for row in matrix if (row[1] != "")]

print(new_matrix)

